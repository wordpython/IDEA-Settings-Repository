<keymap version="1" name="Default copy" parent="$default">
  <action id="CodeCompletion">
    <keyboard-shortcut first-keystroke="ctrl space" />
    <keyboard-shortcut first-keystroke="alt windows" />
  </action>
  <action id="Editor Cut">
    <keyboard-shortcut first-keystroke="ctrl x" />
  </action>
  <action id="EditorDecreaseFontSize">
    <mouse-shortcut keystroke="control button142" />
  </action>
  <action id="EditorIncreaseFontSize">
    <mouse-shortcut keystroke="control button143" />
  </action>
  <action id="FindNext">
    <keyboard-shortcut first-keystroke="f3" />
  </action>
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="ctrl alt l" />
    <keyboard-shortcut first-keystroke="ctrl l" />
  </action>
  <action id="Vcs.Log.FocusTextFilter" />
</keymap>